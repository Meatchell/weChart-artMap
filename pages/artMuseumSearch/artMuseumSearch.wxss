.search-bg  {
    background-color: #CAC9CF;
    width: calc(100% - 30rpx);
    padding:15rpx;
    height: 56rpx;
    border-top:1px solid #B5B5B5;
    border-bottom:1px solid #B5B5B5;
}
.search-bg input {
    width: calc(100% - 100rpx);
    background-color: #fff;
    border-radius: 8rpx;
    height: 56rpx;
    padding-left: 45rpx;
    box-sizing: border-box;
    padding-right:22px;
    display: inline-block;
    vertical-align: middle;
}
.search-bg image {
    width: 34rpx;
    height: 34rpx;
    position: absolute;
    top:25rpx;
    z-index: 3;
}
.search {
    left:25rpx;
}
.remove {
    right: 125rpx;
}
.search-bg text {
    font-size: 16px;
    display: inline-block;
    line-height: 46rpx;
    margin-left: 20rpx;
    color: #7D7C81;
}
.search-result .item {
    width: 100%;
    border-bottom: 1rpx solid #999999;
}
.warp {
    display: flex;
    justify-content: space-between;
    padding:10rpx 20rpx;
}
.left text {
    display: block;
}
.right {
    width: 20%;
    text-align: center;
}
.right .location-image {
    width: 40rpx;
    height: 59rpx;
    background-image: url("http://art-map.oss-cn-beijing.aliyuncs.com/location.png");
    background-size: cover;
    margin-left: calc(50% - 20rpx);
}
.right .text {
    font-size: 12px;
    color: #ccc;
}