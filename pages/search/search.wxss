.search-bg  {
    background-color: #CAC9CF;
    width: calc(100% - 30rpx);
    padding:15rpx;
    height: 56rpx;
    border-top:1px solid #B5B5B5;
    border-bottom:1px solid #B5B5B5;
    position: fixed;
    top:0;
    z-index: 3;
}
.search-bg input {
    width: calc(100% - 100rpx);
    background-color: #fff;
    border-radius: 8rpx;
    height: 56rpx;
    padding-left: 45rpx;
    box-sizing: border-box;
    padding-right:22px;
    display: inline-block;
    vertical-align: middle;
    /*font-size: 14rpx;*/
}
.search-bg image {
    width: 34rpx;
    height: 34rpx;
    position: absolute;
    top:25rpx;
    z-index: 3;
}
.search {
    left:25rpx;
}
.remove {
    right: 125rpx;
}
.search-bg text {
    font-size: 16px;
    display: inline-block;
    line-height: 46rpx;
    margin-left: 20rpx;
    color: #7D7C81;
}
.search-result {
    padding:0 15rpx 30rpx;
    box-sizing: border-box;
    height: calc(100vh - 90rpx);
    margin-top: 90rpx;
    width: 100%;

}

.search-result .warp {
    width: 100%;
    column-gap: 0;
    column-count: 2;
}

.search-result .item {
    /*float: left;*/
    /*display: inline-block;*/
    break-inside: avoid;
    /*box-sizing: border-box; padding: 10px;*/
    /*width: calc(50% - 15rpx);*/
    margin:15rpx;

    /*display: inline-block;*/
}
.search-result image {
    width: 100%;
    /*display: block;*/
}
.search-result text {
    display: block;
}
.search-result .price {
    font-size: 14px;
}
 /*loding部分*/
.loading{
    padding: 10rpx;
    text-align: center;
}
.loading:before{
    display: inline-block;
    margin-right: 5rpx;
    vertical-align: middle;
    content: '';
    width: 40rpx;
    height: 40rpx;
    background: url(http://art-map.oss-cn-beijing.aliyuncs.com/loding.gif) no-repeat;
    background-size: contain;
    animation: rotate 1s linear infinite;
}
.loading.complete:before{
    display: none;
}